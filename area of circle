def distance(cx,cy,px,py):
  dx = px-cx
  dy = py-cy
  distance = (dx**2+dy**2)**0.5
  return distance
  
#function to calculate area of circle
def area(cx,cy,px,py):
  radius = distance(cx,cy,px,py)
  area =  3.14 * radius**2
  return area
  
#first two parameter is of center and last two parameter is on circumference
result = area(0,0,1,1)
print(result)
